uniform float uSlice;

void main()
{
	vec3 p = vec3(gl_FragCoord.x,gl_FragCoord.y,uSlice)/uResolution.x;
	p = p * vec3(2.200000,2.200000,2.200000) + vec3(-1.100000,-1.100000,-1.100000);

	float d = 99999.0;
	d = min(d, fBox(p - vec3(-0.612035,0.576841,0.000000), vec3(0.125000,0.125000,0.593051)));
	d = min(d, fBox(p - vec3(-0.040184,-0.037929,-0.027177), vec3(0.751873,0.125000,0.156539)));
	d = min(d, fBox(p - vec3(-0.040184,-0.547557,-0.027177), vec3(0.751873,0.125000,0.156539)));
	d = min(d, fBox(p - vec3(0.635786,0.580195,0.000000), vec3(0.125000,0.125000,0.250000)));
	d = min(d, fBox(p - vec3(0.000000,0.000000,0.000000), vec3(1.000000,1.000000,0.050000)));
	outColor0=vec4(d);
}
